                                                                sling:resourceSuperType="/apps/apac/components/content/parent/cq:dialog/content/items/layout/items/general/items/column/items/redirectwindow/items"/>
                                                        </linkoptions>
                                                    </items>
                                                </column>
                                            </items>
                                        </field>
                                    </secondarylinks>
                                </items>
                            </column>
                        </items>
                    </secondarylinks>
					 <disclosurelist
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Disclosures"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <disclosureoption
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        checked="{Boolean}false"
                                        name="./disclosureOption"
                                        text="Disclosure Option? "
                                        uncheckedValue="false"
                                        value="true"/>
                                </items>
                            </column>
                        </items>
                    </disclosurelist>
                </items>
            </layout>
        </items>
    </content>
</jcr:root>
