footer.js;
use(function () {
    // you can reference the parameters via the this keyword.
    var hrefValue = this.value;
    var linkOptions = "_self";
    console.log("options ::"+this.value);
    if(this.options=="true"){
       linkOptions = "_blank";
    } else{
        linkOptions = "_self";
    }
	if(hrefValue && hrefValue.indexOf("/content") != -1){
        hrefValue = hrefValue.concat(".html");
    }
    return {
        href : hrefValue,
        target : linkOptions
    };
});
